<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/ontologies-ULL/CO-META#"
     xml:base="https://w3id.org/ontologies-ULL/CO-META"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://creativecommons.org/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:CO-META="https://w3id.org/ontologies-ULL/CO-META#">
    <owl:Ontology rdf:about="https://w3id.org/ontologies-ULL/CO-META">
        <owl:versionIRI rdf:resource="https://w3id.org/ontologies-ULL/CO-META/1.0"/>
        <ns:license rdf:resource="http://creativecommons.org/licenses/by-nc-sa/4.0/"/>
        <dc:creator>Gara Miranda Valladares (http://orcid.org/0000-0002-2181-0067)</dc:creator>
        <dc:creator>Iván Castilla Rodríguez (http://orcid.org/0000-0003-3933-2582)</dc:creator>
        <dc:creator>Miguel Luna García (http://orcid.org/0009-0006-6611-4360)</dc:creator>
        <dc:description xml:lang="en">Ontology for Combinatory Optimization Metaheuristics</dc:description>
        <dc:description xml:lang="es">Ontología para Metaheurísticas de Optimización Combinatoria</dc:description>
        <dc:source rdf:resource="https://github.com/ontologies-ULL/CO-META"/>
        <terms:abstract xml:lang="es">Esta ontología proporciona un marco estructurado para la interoperabilidad de fuentes de datos y aplicaciones en el contexto de las metaheurísticas de optimización combinatoria.</terms:abstract>
        <terms:abstract xml:lang="en">This ontology provides a structured framework for the interoperability of data sources and applications in the context of combinatory optimization metaheuristics.</terms:abstract>
        <rdfs:label xml:lang="en">CO-META</rdfs:label>
        <rdfs:label xml:lang="es">CO-META</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://creativecommons.org/ns#license -->


    <owl:AnnotationProperty rdf:about="http://creativecommons.org/ns#license"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/source -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/source"/>
    


    <!-- http://purl.org/dc/terms/abstract -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/abstract"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasCodification -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasCodification">
        <owl:inverseOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isCodificationOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ontologies-ULL/CO-META#Metaheuristic"/>
                    <rdf:Description rdf:about="https://w3id.org/ontologies-ULL/CO-META#Problem"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Solution"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasDataStructure -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasDataStructure">
        <rdfs:domain rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Metaheuristic"/>
        <rdfs:range rdf:resource="https://w3id.org/ontologies-ULL/CO-META#DataStructure"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasInput -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasInput">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isConnectedTo"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#hasOutput"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasInputEnd -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasInputEnd">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#hasInput"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#hasOutputEnd"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasInputStart -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasInputStart">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#hasInput"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#hasOutputStart"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasNodeType -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasNodeType">
        <rdfs:domain rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Solution"/>
        <rdfs:range rdf:resource="https://w3id.org/ontologies-ULL/CO-META#SolutionNode"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasObjective -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasObjective">
        <owl:inverseOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isObjectiveOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Problem"/>
        <rdfs:range rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Objective"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasOutput -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasOutput">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isConnectedTo"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MetaheuristicComponent"/>
        <rdfs:range rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MetaheuristicComponent"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasOutputEnd -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasOutputEnd">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#hasOutput"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontologies-ULL/CO-META#LoopComponent"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasOutputStart -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasOutputStart">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#hasOutput"/>
        <rdfs:range rdf:resource="https://w3id.org/ontologies-ULL/CO-META#LoopComponent"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasParameter -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasParameter">
        <owl:inverseOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isParameterOf"/>
        <rdfs:range rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Parameter"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasRestriction -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasRestriction">
        <owl:inverseOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isRestrictionOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Problem"/>
        <rdfs:range rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Restriction"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasStart -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasStart">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isConnectedTo"/>
        <owl:inverseOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isStartOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Metaheuristic"/>
        <rdfs:range rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MetaheuristicComponent"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#isCodificationOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#isCodificationOf"/>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#isConnectedTo -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#isConnectedTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasOutput"/>
            <rdf:Description rdf:about="https://w3id.org/ontologies-ULL/CO-META#isConnectedTo"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#isObjectiveOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#isObjectiveOf"/>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#isParameterOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#isParameterOf"/>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#isRestrictionOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#isRestrictionOf"/>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#isStartOf -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#isStartOf">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isConnectedTo"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#readsData -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#readsData">
        <rdfs:domain rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MetaheuristicComponent"/>
        <rdfs:range rdf:resource="https://w3id.org/ontologies-ULL/CO-META#DataStructure"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#writesData -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#writesData">
        <rdfs:domain rdf:resource="https://w3id.org/ontologies-ULL/CO-META#DataHandlerComponent"/>
        <rdfs:range rdf:resource="https://w3id.org/ontologies-ULL/CO-META#DataStructure"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/ontologies-ULL/CO-META#hasSize -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#hasSize">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Solution"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#maxValue -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#maxValue">
        <rdfs:domain rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Parameter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#minValue -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/ontologies-ULL/CO-META#minValue">
        <rdfs:domain rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Parameter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/ontologies-ULL/CO-META#AcceptanceComponent -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#AcceptanceComponent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MetaheuristicComponent"/>
        <rdfs:comment xml:lang="en">Accepts a solution out of two or more solutions, according to its fitness. Needs fitness assingment component in multiobjective optimization</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#Bit -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#Bit">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#SolutionNode"/>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#BoolParameter -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#BoolParameter">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Parameter"/>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#ConstructionComponent -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#ConstructionComponent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MetaheuristicComponent"/>
        <rdfs:comment xml:lang="en">Constructs a solution or a group of solutions using heuristic methods</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#ConstructiveMetaheuristic -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#ConstructiveMetaheuristic">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ontologies-ULL/CO-META#Metaheuristic"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isConnectedTo"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/ontologies-ULL/CO-META#ConstructionComponent"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#CrossoverComponent -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#CrossoverComponent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#ModificationComponent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ontologies-ULL/CO-META#hasParameter"/>
                <owl:hasValue rdf:resource="https://w3id.org/ontologies-ULL/CO-META#CrossoverProbability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Production of one or more individuals from two or more parent individuals</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#CustomSet -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#CustomSet">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#SolutionNode"/>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#DataHandlerComponent -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#DataHandlerComponent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MetaheuristicComponent"/>
        <rdfs:comment xml:lang="en">Updates specific data structures</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#DataStructure -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#DataStructure">
        <rdfs:comment xml:lang="en">Data structure that stores information used by the algorithm</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#DiversityPreservationComponent -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#DiversityPreservationComponent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MultiobjectiveComponent"/>
        <rdfs:comment xml:lang="en">Adjust the fitness value to encourage diversity in the population</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#EvolutionaryAlgorithm -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#EvolutionaryAlgorithm">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ontologies-ULL/CO-META#PopulationBasedMetaheuristic"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isConnectedTo"/>
                                <owl:someValuesFrom rdf:resource="https://w3id.org/ontologies-ULL/CO-META#CrossoverComponent"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isConnectedTo"/>
                                <owl:someValuesFrom rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MutationComponent"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isConnectedTo"/>
                                <owl:someValuesFrom rdf:resource="https://w3id.org/ontologies-ULL/CO-META#SelectionComponent"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#FitnessAssingmentComponent -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#FitnessAssingmentComponent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MultiobjectiveComponent"/>
        <rdfs:comment>Assign an individual fitness value to each member of a population</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#FloatParameter -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#FloatParameter">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Parameter"/>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#GenerationComponent -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#GenerationComponent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MetaheuristicComponent"/>
        <rdfs:comment xml:lang="en">Generates an initial solution or population</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#Integer -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#Integer">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#SolutionNode"/>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#IntegerParameter -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#IntegerParameter">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Parameter"/>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#List -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#List">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Solution"/>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#LocalSearchComponent -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#LocalSearchComponent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#ModificationComponent"/>
        <rdfs:comment xml:lang="en">Improves the solution fitness by exploring its neighborhood</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#LocalSearchMetaheuristic -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#LocalSearchMetaheuristic">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ontologies-ULL/CO-META#Metaheuristic"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isConnectedTo"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/ontologies-ULL/CO-META#GenerationComponent"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#LoopComponent -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#LoopComponent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MetaheuristicComponent"/>
        <rdfs:comment xml:lang="en">Creates loops to iterate through data structures or repeat processes until a condition is met. Has two additional properties: hasOutputStart and hasOutputEnd</rdfs:comment>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#MaximizationObjective -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#MaximizationObjective">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontologies-ULL/CO-META#Objective"/>
        <owl:disjointWith rdf:resource="https://w3id.org/ontologies-ULL/CO-META#MinimizationObjective"/>
    </owl:Class>
    


    <!-- https://w3id.org/ontologies-ULL/CO-META#MemeticAlgorithm -->


    <owl:Class rdf:about="https://w3id.org/ontologies-ULL/CO-META#MemeticAlgorithm">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ontologies-ULL/CO-META#EvolutionaryAlgorithm"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/ontologies-ULL/CO-META#isConnectedTo"/>
